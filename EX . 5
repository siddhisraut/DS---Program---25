#include <iostream>
using namespace std;

#define MAX 5

class Stack {
    int arr[MAX];
    int top;

public:
    Stack() {
        top = -1;
    }

    void push() {
        int x;
        if (top == MAX - 1) {
            cout << "Stack Overflow\n";
            return;
        }
        cout << "Enter element: ";
        cin >> x;
        arr[++top] = x;
    }

    void pop() {
        if (top == -1) {
            cout << "Stack Underflow\n";
            return;
        }
        cout << "Deleted element: " << arr[top--] << endl;
    }

    void display() {
        if (top == -1) {
            cout << "Stack Empty\n";
            return;
        }
        cout << "Stack elements: ";
        for (int i = top; i >= 0; i--)
            cout << arr[i] << " ";
        cout << endl;
    }
};

int main() {

    cout << "Name  : Siddhi Sachin Raut\n";
    cout << "Roll No: 25\n";
    cout << "Class : S.Y. B. CSE\n";

    Stack s;
    int choice;

    do {
        cout << "\n1.Push\n2.Pop\n3.Display\n4.Exit\nEnter choice: ";
        cin >> choice;

        switch(choice) {
            case 1: s.push(); break;
            case 2: s.pop(); break;
            case 3: s.display(); break;
            case 4: cout << "Exiting...\n"; break;
            default: cout << "Invalid choice\n";
        }
    } while(choice != 4);

    return 0;
}
